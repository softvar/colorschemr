{
  "name": "color-schemr",
  "version": "1.0.0",
  "description": "Color Schemer",
  "scripts": {
    "typings-install": "node node_modules/typings/dist/bin.js install",
    "postinstall": "npm run typings-install",
    "build": "node node_modules/webpack/bin/webpack.js --inline --colors --progress --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "server": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src",
    "start": "npm run server",
    "tslint": "tslint",
    "lint": "npm run tslint 'src/**/*.ts'"
  },
  "contributors": [
    "Varun Malhotra <varun2902@gmail.com>",
    "Gaurav Nanda <93.gaurav@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "awesome-typescript-loader": "~0.16.2",
    "es6-promise": "3.0.2",
    "es6-shim": "0.35.0",
    "reflect-metadata": "0.1.3",
    "source-map-loader": "^0.1.5",
    "typescript": "~1.8.9",
    "typings": "^0.7.9",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.8.4",
    "tslint": "^3.7.1",
    "tslint-loader": "^2.1.3"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    "@angular/forms": "2.0.0",
    "core-js": "^2.4.1",
    "material-design-lite": "^1.2.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "~0.6.17"
  },
  "keywords": [
    "Angular2",
    "angular2-webpack",
    "angular2-app",
    "color scheme",
    "color schemer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rumblex/angularattack2016-colorschemr"
  },
  "bugs": {
    "url": "https://github.com/rumblex/angularattack2016-colorschemr/issues"
  },
  "homepage": "http://colorschemr.2016.angularattack.io/"
}
